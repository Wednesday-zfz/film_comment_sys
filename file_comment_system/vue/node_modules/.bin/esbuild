#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yuyaobo/Course/大三上/数据库/film_comment_sys/file_comment_system/vue/node_modules/esbuild/bin/node_modules:/Users/yuyaobo/Course/大三上/数据库/film_comment_sys/file_comment_system/vue/node_modules/esbuild/node_modules:/Users/yuyaobo/Course/大三上/数据库/film_comment_sys/file_comment_system/vue/node_modules:/Users/yuyaobo/Course/大三上/数据库/film_comment_sys/file_comment_system/node_modules:/Users/yuyaobo/Course/大三上/数据库/film_comment_sys/node_modules:/Users/yuyaobo/Course/大三上/数据库/node_modules:/Users/yuyaobo/Course/大三上/node_modules:/Users/yuyaobo/Course/node_modules:/Users/yuyaobo/node_modules:/Users/node_modules:/node_modules:/Users/yuyaobo/Course/大三上/数据库/film_comment_sys/file_comment_system/vue/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yuyaobo/Course/大三上/数据库/film_comment_sys/file_comment_system/vue/node_modules/esbuild/bin/node_modules:/Users/yuyaobo/Course/大三上/数据库/film_comment_sys/file_comment_system/vue/node_modules/esbuild/node_modules:/Users/yuyaobo/Course/大三上/数据库/film_comment_sys/file_comment_system/vue/node_modules:/Users/yuyaobo/Course/大三上/数据库/film_comment_sys/file_comment_system/node_modules:/Users/yuyaobo/Course/大三上/数据库/film_comment_sys/node_modules:/Users/yuyaobo/Course/大三上/数据库/node_modules:/Users/yuyaobo/Course/大三上/node_modules:/Users/yuyaobo/Course/node_modules:/Users/yuyaobo/node_modules:/Users/node_modules:/node_modules:/Users/yuyaobo/Course/大三上/数据库/film_comment_sys/file_comment_system/vue/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../esbuild/bin/esbuild" "$@"
else
  exec node  "$basedir/../esbuild/bin/esbuild" "$@"
fi
